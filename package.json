{
  "name": "ngpack",
  "version": "1.1.0",
  "description": "AngularJS + Webpack + CSS-modules + Baobab",
  "main": "index.js",
  "scripts": {
    "test": "webpack",
    "clean": "rm -rf build",
    "devmode": "set NODE_ENV=development",
    "dev": "npm run devmode && webpack-dev-server --content-base src/ --hot --history-api-fallback --inline --progress --port 8081",
    "build": "npm run clean && set NODE_ENV=production && webpack -p"
