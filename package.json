    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
